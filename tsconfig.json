{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "stripInternal": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es5",
    "lib": ["es2015", "dom"],
    "types": [],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "importHelpers": true,
    "outDir": "dist",
    "paths": {
      "@angular/core": ["node_modules/@angular/core"],
      "rxjs/*": ["node_modules/rxjs/*"],
      "@abp/*": ["./src/abp/*"],
      "yoyo-ng-module": ["./src/"],
      "yoyo-ng-module/*": ["./src/*"]
    }
  },
  "files": [
    "index.ts",
    "src/index.ts",
    "src/abp/index.ts",
    "src/abc/index.ts",
    "src/theme/index.ts",
    "src/util/index.ts",
    "src/typings.d.ts",
    "./node_modules/abp-web-resources/Abp/Framework/scripts/abp.d.ts"
  ],
  "angularCompilerOptions": {
    "strictMetadataEmit": true
  }
}